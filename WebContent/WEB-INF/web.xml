<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	<display-name>mysite</display-name>
	<servlet>
		<servlet-name>GetEMailAddressDtlsServlet</servlet-name>
		<servlet-class>
			assign.servlets.GetEMailAddressDtlsServlet
		</servlet-class>
	</servlet>


	<servlet>
		<servlet-name>GetAllEMailListServlet</servlet-name>
		<servlet-class>
			assign.servlets.GetAllEMailListServlet
		</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AddEmailAddressServlet</servlet-name>
		<servlet-class>
			assign.servlets.AddEMailAddressServlet
		</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DeleteEmailAddressServlet</servlet-name>
		<servlet-class>
			assign.servlets.DeleteEMailAddressServlet
		</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>GetGroupDtlsServlet</servlet-name>
		<servlet-class>
			assign.servlets.GetGroupDtlsServlet
		</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>GetAllEMailListbyGroupServlet</servlet-name>
		<servlet-class>
			assign.servlets.GetAllEMailListbyGroupServlet
		</servlet-class>
	</servlet>
	
	<servlet> 
		<description>
		</description>
		<display-name>
		GetEmailAddresswithAjax</display-name>
		<servlet-name>GetEmailAddresswithAjax</servlet-name>
		<servlet-class>
		assign.servlets.GetEmailAddresswithAjax</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>GetEMailAddressDtlsModifyServlet</display-name>
		<servlet-name>GetEMailAddressDtlsModifyServlet</servlet-name>
		<servlet-class>assign.servlets.GetEMailAddressDtlsModifyServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>GetEMailAddressDtlsServlet</servlet-name>
		<url-pattern>/servlet/emailaddressdtls</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>AddEmailAddressServlet</servlet-name>
		<url-pattern>/servlet/addemailaddress</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>DeleteEmailAddressServlet</servlet-name>
		<url-pattern>/servlet/deleteemailaddress</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>GetGroupDtlsServlet</servlet-name>
		<url-pattern>/servlet/getgroupdtlsservlet</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>GetAllEMailListbyGroupServlet</servlet-name>
		<url-pattern>/servlet/getallemaillistbygroup</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>GetAllEMailListServlet</servlet-name>
		<url-pattern>/servlet/viewall</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>GetEmailAddresswithAjax</servlet-name>
		<url-pattern>/getemailaddresswithAjax</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>GetEMailAddressDtlsModifyServlet</servlet-name>
		<url-pattern>/GetEMailAddressDtlsModifyServlet</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
</web-app>
